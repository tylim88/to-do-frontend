{
    "name": "todo",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "babel-plugin-emotion": "^10.0.7",
        "bootstrap": "^4.2.1",
        "jsonwebtoken": "^8.4.0",
        "react": "^16.7.0",
        "react-bootstrap": "^1.0.0-beta.5",
        "react-dom": "^16.7.0",
        "react-router-dom": "^4.3.1",
        "react-scripts": "2.1.3",
        "react-spinners": "^0.5.1",
        "source-map-explorer": "^1.6.0",
        "superagent": "^4.1.0",
        "unstated": "^2.1.1",
        "validator": "^10.11.0"
    },
    "scripts": {
        "dev": "env-cmd ./.config/dev.env react-scripts start",
        "dbuild": "env-cmd ./.config/dev.env react-scripts build",
        "start": "env-cmd ./.config/prod.env react-scripts start",
        "build": "env-cmd ./.config/prod.env react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "analyze": "npm run build && source-map-explorer build/static/js/main.*"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "env-cmd": "^8.0.2"
    },
    "babel": {
        "presets": [
            "react",
            "env"
        ],
        "plugins": [
            "emotion"
        ]
    }
}
